bspUsesGDBSimulator="no"
bspNeedsDos2Unix="yes"
runBSP=skyeye

bspExited_ARM()
{
  for pattern in  \
     "^Unhandled exception" \
     "^SKYEYE:Error"
  do
    grep "${pattern}" ${logfile}
    if [ $? -eq 0 ] ; then
      return 1
    fi
  done
  return 0
}
